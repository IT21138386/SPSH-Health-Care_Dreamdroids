<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.spsh.spshhealthcare">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/spsh_health_care_logo_with_background"
        android:label="@string/app_name"
        android:roundIcon="@drawable/spsh_health_care_logo_with_background"
        android:supportsRtl="true"
        android:theme="@style/Theme.SPSHHealthCare"
        tools:targetApi="31">
        <activity
            android:name=".ViewAllReports"
            android:windowSoftInputMode="stateHidden"
            android:exported="false" />
        <activity
            android:name=".ViewReport"
            android:exported="false" />
        <activity
            android:name=".UpdateReport"
            android:exported="false" />
        <activity
            android:name=".AddReport"
            android:exported="false" />
        <activity
            android:name=".LabHome"
            android:exported="false" />
        <activity
            android:name=".PatientSignup"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>