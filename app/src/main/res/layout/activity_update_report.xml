        android:id="@+id/et_labelForWbcUpdateReport"
        android:layout_width="127dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_unitForHemoglobinUpdateReport"
        app:layout_constraintStart_toEndOf="@+id/tv_unitForHemoglobinUpdateReport"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/tv_labelForWbcUpdateReport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text="@string/tv_labelForWbcUpdateReport"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_labelForHimoglobinUpdateReport"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.635"
        app:layout_constraintStart_toEndOf="@+id/tv_labelForHimoglobinUpdateReport" />

    <TextView
        android:id="@+id/tv_labelForHimoglobinUpdateReport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_labelForHimoglobinUpdateReport"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_unitForCostUpdateReport" />

    <TextView
        android:id="@+id/tv_unitForHemoglobinUpdateReport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tv_unitForHemoglobinUpdateReport"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_labelForHimoglobinUpdateReport"
        app:layout_constraintStart_toEndOf="@+id/et_labelForHimoglobinUpdateReport" />

