    <TextView
        android:id="@+id/tv_unitForRbcUpdateReport"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginEnd="16dp"
        android:text="@string/tv_unitForRbcUpdateReport"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_labelForRbcUpdateReport"
