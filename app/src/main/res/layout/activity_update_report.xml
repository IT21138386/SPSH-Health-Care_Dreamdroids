    <EditText
        android:id="@+id/et_labelForLymphocytesUpdateReport"
        android:layout_width="127dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_unitForNeutrophilsUpdateReport"
