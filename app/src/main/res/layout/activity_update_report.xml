        android:id="@+id/tv_unitForWbcUpdateReport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="@string/tv_unitForWbcUpdateReport"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_labelForWbcUpdateReport"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/et_labelForWbcUpdateReport" />

    <TextView
        android:id="@+id/tv_unitForNeutrophilsUpdateReport"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:text="@string/tv_unitForNeutrophilsUpdateReport"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_labelForNeutrophilsUpdateReport"
        app:layout_constraintStart_toEndOf="@+id/et_labelForNeutrophilsUpdateReport" />

    <TextView
        android:id="@+id/tv_unitForLymphocytesUpdateReport"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginEnd="16dp"
        android:text="@string/tv_unitForLymphocytesUpdateReport"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_labelForLymphocytesUpdateReport"
        app:layout_constraintEnd_toEndOf="parent"
