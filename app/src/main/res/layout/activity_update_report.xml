        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/et_labelForRbcUpdateReport" />

    <TextView
        android:id="@+id/tv_unitForPcbUpdateReport"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:text="@string/tv_unitForPcbUpdateReport"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_labelForPcbUpdateReport"
