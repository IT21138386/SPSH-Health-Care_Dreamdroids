        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text="@string/tv_labelForRbcUpdateReport"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_labelForEosinophilsUpdateReport"
